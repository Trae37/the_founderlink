{
  "buildCommand": "npm run build",
  "outputDirectory": "dist/public",
  "framework": null,
  "rewrites": [{ "source": "/api/(.*)", "destination": "/api/index.ts" }],
  "functions": {
    "api/index.ts": {
      "maxDuration": 30,
      "includeFiles": [
        "server/**",
        "shared/**",
        "drizzle/**",
        "generated-documents/**"
      ]
    }
  }
}
